{{#each rounds as |round|}}

  <ul class="rounds-list" style="list-style: none;" data-id="{{round.id}}">
  <li><strong>Date:</strong> {{round.date}}</li>
  <li><strong>Range Name:</strong> {{round.range_name}}</li>
  <li><strong>Range Type:</strong> {{round.range_type}}</li>
  <li><strong>Bow Class:</strong> {{round.bow_class}}</li>
  <li><strong>Arrows/End:</strong> {{round.arrows_per_end}}</li>
  <li><strong># of Ends:</strong> {{round.number_of_ends}}</li>
  <li><strong>Total Score:</strong> {{round.total_score}}</li>
  <button class="editRoundButton" data-id="{{round.id}}">Edit</button>
  <button class="deleteRoundButton" data-id="{{round.id}}">Delete</button>
  </ul>

{{/each}}
